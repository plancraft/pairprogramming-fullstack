{
  "name": "pairprogramming-fullstack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "pkgs/*"
  ],
  "scripts": {
    "dev": "nx run frontend:dev",
    "server": "nx run backend:dev",
    "dev:all": "nx run-many -t dev --projects frontend,backend --parallel",
